<h1>Stages</h1>
<%= link_to 'Add Stage', new_venue_path %><br><br>
<% @venues.each do |venue| %>
<%= link_to venue.name, venue_path(venue) %>
<%= link_to 'Edit', edit_venue_path(venue) %>
<%= link_to 'Delete', venue_path(venue), method: :delete, data: {confirm: 'Are you sure you want to delete this venue and all of its time slots?'} %><br><br>
<% end %>